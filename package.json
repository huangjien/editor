{
  "name": "editor",
  "version": "0.1.13",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "dev": "npm run start",
    "dev:reset": "npm run start -- --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --mode Release",
    "build:ios-archive": "cd ios && xcodebuild -workspace editor.xcworkspace -scheme editor -configuration Release archive -archivePath build/editor.xcarchive",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:unit:watch": "jest --testPathIgnorePatterns=e2e --watch",
    "test:unit:coverage": "jest --testPathIgnorePatterns=e2e --coverage",
    "test:unit:verbose": "jest --testPathIgnorePatterns=e2e --verbose",
    "test:all": "npm run test:unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:clear": "jest --clearCache",
    "test:file": "jest --testNamePattern",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "npm run test:clear && cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "clean:install": "npm run clean && npm install && cd ios && pod install",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-vector-icons/ionicons": "^12.3.0",
    "@react-native/new-app-screen": "0.81.4",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "axios": "^1.12.2",
    "i18next": "^25.5.2",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.4",
    "react-native-audio-pro": "^10.1.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-keychain": "^10.0.0",
    "react-native-localize": "^3.5.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-track-player": "^4.1.2",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
